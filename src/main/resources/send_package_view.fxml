<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.VBox?>

<VBox alignment="CENTER" spacing="10.0" xmlns:fx="http://javafx.com/fxml" fx:controller="controllers.SendPackageController">
    <ComboBox fx:id="deliveryCenterComboBox" promptText="Select Delivery Center"/>
    <ComboBox fx:id="packageTypeComboBox" promptText="Select Package Type"/>
    <TextField fx:id="receiverIdField" promptText="Receiver ID"/>
    <TextField fx:id="weightField" promptText="Weight"/>
    <Button fx:id="sendPackageButton" text="Send Package"/>
    <Button fx:id="backButton" text="Back"/>
</VBox>
