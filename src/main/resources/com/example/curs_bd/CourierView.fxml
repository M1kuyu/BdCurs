<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<GridPane fx:controller="com.example.controllers.CourierController" xmlns:fx="http://javafx.com/fxml" alignment="CENTER" hgap="10" vgap="10">
    <padding>
        <Insets top="10" right="10" bottom="10" left="10"/>
    </padding>

    <Label text="Центр доставки:" GridPane.columnIndex="0" GridPane.rowIndex="0"/>
    <ComboBox fx:id="centerComboBox" GridPane.columnIndex="1" GridPane.rowIndex="0"/>

    <Button text="Загрузить посылки" onAction="#handleLoadButtonAction" GridPane.columnIndex="1" GridPane.rowIndex="1"/>
    <Button text="Назад" onAction="#handleBackButtonAction" GridPane.columnIndex="1" GridPane.rowIndex="2"/>
</GridPane>
