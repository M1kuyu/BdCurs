<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.CheckBox?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.VBox?>

<VBox alignment="CENTER" prefHeight="311.0" prefWidth="256.0" spacing="10.0" xmlns:fx="http://javafx.com/fxml" fx:controller="controllers.RegisterEmployeeController">
    <TextField fx:id="nameField" promptText="Name" />
    <TextField fx:id="phoneField" promptText="Phone" />
    <TextField fx:id="loginField" promptText="Login" />
    <TextField fx:id="passwordField" promptText="Password" />
    <CheckBox fx:id="isAdminCheckBox" text="Register as Admin" />
    <ComboBox fx:id="deliveryCenterComboBox" promptText="Select Delivery Center" />
    <Button fx:id="registerButton" text="Register" />
    <Button fx:id="backButton" text="Back" />
</VBox>
